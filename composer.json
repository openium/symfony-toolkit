{
    "name": "openium/symfony-toolkit",
    "description": "Openium Web Toolkit for Symfony Project",
    "keywords": [
        "symfony",
        "toolkit",
        "openium"
    ],
    "type": "symfony-bundle",
    "license": "MIT",
    "homepage" : "https://www.openium.fr",
    "authors": [
        {
            "name": "Thomas Leduc",
            "email": "thomaslmoi15@hotmail.fr"
        },
        {
            "name": "Alexandre Caillot (Shiroe_sama)",
            "email": "a.caillot@openium.fr"
        }
    ],
    "autoload": {
        "psr-4": {
            "Openium\\SymfonyToolKitBundle\\": ""
        },
        "exclude-from-classmap": [
            "Tests/"
        ]
    },
    "require": {
        "php": "^7.1",
        "symfony/http-foundation": "^4.1",
        "doctrine/orm": "^2.6",
        "symfony/validator": "^4.1",
        "symfony/http-kernel": "^4.1",
        "symfony/dependency-injection": "^4.1",
        "symfony/config": "^4.1",
        "symfony/monolog-bundle": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^6",
        "symfony/phpunit-bridge": "^4.1"
    }
}
